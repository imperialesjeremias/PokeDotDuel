{
  "name": "pokebattle",
  "private": true,
  "scripts": {
    "dev": "node start-project.js",
    "dev:ws": "cd apps/websocket-server && npm run dev",
    "build": "npm run build:backend && npm run build:frontend && npm run build:ws",
    "build:backend": "cd apps/backend && npm run build",
    "build:frontend": "cd apps/web && npm run build",
    "build:ws": "cd apps/websocket-server && npm run build",
    "start": "node start-project.js",
    "start:ws": "cd apps/websocket-server && npm run start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "setup": "node setup-env.js",
    "verify": "node verify-implementation.js"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "npm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "programs/*"
  ]
}
